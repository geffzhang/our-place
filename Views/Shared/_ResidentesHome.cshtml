    <script>
        function MostrarError(result) {
            if (result.error) {
                if (result.status == "400"
                    || result.status == "401"
                    || result.status == "403"
                    || result.status == "404"
                    || result.status == "500"
                    || result.status == "403") {
                    $.ajax({
                        url: '/Inicio/Error' + result.status,
                        cache: false,
                        dataType: "html",
                        success: function (data) {
                            document.getElementById("ContentOnePage").innerHTML = data;
                        }
                    });
                }
                else {
                    document.getElementById("ContentOnePage").innerHTML = result.responseText;
                }

