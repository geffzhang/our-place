        function FormatMoneyAllClassmoney() {
            var inputs = document.getElementsByClassName("money");
            for (x = 0 ; x < inputs.length; x++) {
                FormatMoney(inputs[x]);
            }
        }
        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode
